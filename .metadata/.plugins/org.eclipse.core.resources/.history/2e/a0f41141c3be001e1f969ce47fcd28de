package model;

public class PaymentProcessor {

	public void processPayment(PayementMethod payementMethod,double amount) {
		switch (payementMethod) {
		case CreditCard cc -> {
			cc.welcomeMessage();
			cc.creditMethod();
		}
		case DebitCard dc ->{
			dc.welcomeMessage();
			dc.debitMethod();
		}
		default->System.out.println();
			
		}
	}
}
